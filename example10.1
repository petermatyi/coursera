import re
hand = open('mbox-short.txt')
hrs = dict()

# Read line-by-line
for line in hand:
    line = line.rstrip()
    x = re.findall('^From .* ([0-9][0-9]):', line)
    if len(x) > 0:
        for h in x:
            hrs[h] = hrs.get(h, 0) + 1

